{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "chrome >= 58",
            "firefox >= 53"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread",
    [
    "babel-plugin-root-import",
      {
        "braveSuffix": "./",
        "bravePrefix": "~"
      }
    ]
  ]
}
